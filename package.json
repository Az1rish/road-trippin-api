{
  "name": "road-trippin-api",
  "version": "1.0.0",
  "description": "Helping everyone see the world",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate -- 0 && npm run migrate",   
    "start": "nodemon src/server.js",
    "predeploy": "npm audit && npm run migrate:production", 
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "engines": {
    "node": "10.16.0"
  },
  "author": "Shane McNeil <shanemcneil19@gmail.com",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.600.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^7.17.0",
    "treeize": "^2.1.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  }
}
